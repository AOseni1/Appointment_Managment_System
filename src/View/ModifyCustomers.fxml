<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModifyCustomers">
<children>
    <VBox layoutY="-1.0" prefHeight="400.0" prefWidth="600.0">
        <children>
            <GridPane vgap="30.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="143.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="457.0" minWidth="10.0" prefWidth="457.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <Label text="Customer ID:" />
                    <Label text="Customer Name:" GridPane.rowIndex="1" />
                    <Label text="Address:" GridPane.rowIndex="2" />
                    <Label text="Postal Code:" GridPane.rowIndex="3" />
                    <Label text="Phone Number:" GridPane.rowIndex="4" />
                    <Label text="Countries" GridPane.rowIndex="5" />
                    <TextField fx:id="modifyCustomersCustomerIDTextField" prefHeight="27.0" prefWidth="357.0" GridPane.columnIndex="1" />
                    <TextField fx:id="modifyCustomersCustomerNameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <TextField fx:id="modifyCustomerAddressTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    <TextField fx:id="modifyCustomersPostalCodeTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                    <TextField fx:id="modifyCustomersPhoneNumberTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                    <Label text="Divisions" GridPane.rowIndex="6" />
                    <ChoiceBox fx:id="modifyCustomersCountriesDropDownMenu" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                    <ChoiceBox fx:id="modifyCustomersDivisionsDropDownMenu" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                </children>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                <children>
                    <Button fx:id="ModifyCustomersSaveChangesButton" mnemonicParsing="false" onAction="#onActionSaveChanges" prefHeight="27.0" prefWidth="123.0" text="Save Changes" />
                    <Button fx:id="ModifyCustomersDeleteChangesButton" mnemonicParsing="false" onAction="#onActionDeleteCustomer" prefHeight="27.0" prefWidth="123.0" text="Delete Customer" />
                  <Button fx:id="ModifyCustomersCancelButton" mnemonicParsing="false" onAction="#onActionCancel" prefWidth="123.0" text="Cancel" />
                </children>
                <VBox.margin>
                    <Insets top="10.0" />
                </VBox.margin>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </HBox>
        </children>
        <padding>
            <Insets left="10.0" top="20.0" />
        </padding>
    </VBox>
</children>
        </AnchorPane>
